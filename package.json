  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/uze-platform/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "node --experimental-vm-modules $(yarn bin jest) --forceExit --logHeapUsage",
    "test:clearCache": "test --clearCache",
    "test:openHandles": "node --experimental-vm-modules $(yarn bin jest) --detectOpenHandles --forceExit --logHeapUsage -i",
    "test:watch": "test --watch",
    "test:cov": "node --experimental-vm-modules $(yarn bin jest) --coverage --ci --reporters=default --reporters=jest-junit --forceExit --maxWorkers 2 --logHeapUsage",
    "test:debug": "node --experimental-vm-modules --inspect-brk -r tsconfig-paths/register -r ts-node/register $(yarn bin jest) --detectOpenHandles",
    "test:e2e": "test --config ./apps/marketplace-api/test/jest-e2e.json",
    "prepare": "ts-patch install && husky install",
    "pre-commit": "lint-staged"
  },
