  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.spec.json",
          "useESM": true
        }
      ]
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(file-type|strtok3|peek-readable|token-types|jest-runtime|reflect-metadata))",
      "\\.pnp\\.[^\\/]+$"
    ],
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ],
    "moduleNameMapper": {
      "^(.{1,2}/.*)\\.js$": "$1"
    }
  },
  "packageManager": "yarn@4.0.0-rc.46"
